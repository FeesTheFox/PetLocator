<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#FDC59C" />
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"
        android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#FAD7BE" />
            <size
                android:width="80dp"
                android:height="80dp" />
        </shape>
    </item>
</layer-list>
